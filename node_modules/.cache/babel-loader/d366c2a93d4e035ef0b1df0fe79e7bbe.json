\" v-model=\"searchValue\" @keyup.enter.native=\"searchIdle\">\n\t\t\t\t\t\t\t<el-button slot=\"append\" icon=\"el-icon-search\" @click=\"searchIdle\"></el-button>\n\t\t\t\t\t\t</el-input>\n\t\t\t\t\t</span>\n        <el-table\n                :data=\"Order\"\n                stripe\n                style=\"width: 100%;color: #5a5c61;\"\n                @row-dblclick=\"showOrderDetails\">\n            <el-table-column\n                    prop=\"orderNumber\"\n                    label=\"订单号\"\n                    show-overflow-tooltip\n                    width=\"200\">\n            </el-table-column>\n            <el-table-column\n                    prop=\"idleItem.idleName\"\n                    label=\"闲置名称\"\n                    show-overflow-tooltip\n                    >\n            </el-table-column>\n            <el-table-column\n                    prop=\"orderPrice\"\n                    label=\"金额\"\n                    show-overflow-tooltip\n                    min-width=\"100\"\n                    width=\"100\">\n            </el-table-column>\n            <el-table-column\n                    prop=\"createTime\"\n                    label=\"创建时间\"\n                    show-overflow-tooltip\n                    min-width=\"150\"\n                    width=\"200\">\n            </el-table-column>\n            <el-table-column\n                    label=\"订单状态\"\n                    width=\"100\"\n                    show-overflow-tooltip>\n                <template slot-scope=\"scope\">\n                    <div>{{orderStatus[scope.row.orderStatus]}}</div>\n                </template>\n            </el-table-column>\n            <el-table-column\n                    label=\"支付状态\"\n                    width=\"100\"\n                    show-overflow-tooltip\n                    class=\"cell el-tooltip\">\n                <template slot-scope=\"scope\">\n                    <div>{{paymentStatus[scope.row.paymentStatus]}}</div>\n                </template>\n            </el-table-column>\n            <el-table-column\n                    prop=\"paymentWay\"\n                    label=\"支付方式\"\n                    width=\"100\"\n                    show-overflow-tooltip>\n            </el-table-column>\n            <el-table-column\n                    prop=\"paymentTime\"\n                    label=\"支付时间\"\n                    show-overflow-tooltip>\n            </el-table-column>\n            <el-table-column label=\"操作\">\n                <template slot-scope=\"scope\">\n                    <el-button\n                            size=\"mini\"\n                            type=\"danger\"\n                            @click=\"confirmDelete(scope.$index)\">删除</el-button>\n                </template>\n            </el-table-column>\n        </el-table>\n\n        <!-- 确认弹窗 -->\n        <el-dialog\n            :title=\"'确认删除'\"\n            :visible.sync=\"dialogDeleteVisible\"\n            width=\"30%\"\n            center>\n            <p>您确定要删除这个订单吗？</p>\n            <div slot=\"footer\">\n                <el-button @click=\"dialogDeleteVisible = false\">取消</el-button>\n                <el-button type=\"primary\" @click=\"deleteOrder(targetIndex)\">确定</el-button>\n            </div>\n        </el-dialog>\n        <el-dialog\n            :visible.sync=\"dialogVisible\"\n            title=\"订单详情\"\n            width=\"50%\">\n            <div v-if=\"selectedRow\">\n                <p>订单号: {{ selectedRow.orderNumber }}</p>\n                <p>闲置名称: {{ selectedRow.idleItem.idleName }}</p>\n                <p>金额: {{ selectedRow.orderPrice }}</p>\n                <p>创建时间: {{ selectedRow.createTime }}</p>\n                <p>订单状态: {{ orderStatus[selectedRow.orderStatus] }}</p>\n                <p>支付状态: {{ paymentStatus[selectedRow.paymentStatus] }}</p>\n                <p>支付方式: {{ selectedRow.paymentWay }}</p>\n                <p>支付时间: {{ selectedRow.paymentTime }}</p>\n            </div>\n            <div slot=\"foot