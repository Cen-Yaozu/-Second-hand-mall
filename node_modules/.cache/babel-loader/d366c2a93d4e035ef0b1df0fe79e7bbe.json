           })\n            },\n            handleCurrentChange(val) {\n                this.nowPage = val;\n                this.getOrder();\n            },\n            searchIdle(){\n                this.$api.queryOrder({\n                    page: this.nowPage,\n                    nums: 8,\n                    searchValue: this.searchValue\n                }).then(res => {\n                    console.log(res);\n                    if (res.status_code == 1) {\n                        this.Order = res.data.list;\n                        this.total = res.data.count;\n                    } else{\n                        this.$message.error(res.msg)\n                    }\n                }).catch(e => {\n                    console.log(e)\n                })\n            }\n        },\n        data(){\n            return {\n                mode:1,\n                dialogDeleteVisible: false,\n                targetIndex: null,\n                nowPage: 1,\n                total: 0,\n                paymentStatus:['未支付','已支付'],\n                orderStatus:['待付款','待发货','待收货','已完成','已取消'],\n                Order: [],\n                searchValue: '',\n                dialogVisible: false,\n                selectedRow: null,\n            }\n        },\n    }\n</script>\n\n<style scoped>\n    .main-border{\n        background-color: #FFF;\n        padding: 10px 30px;\n        box-shadow: 0 1px 15px -6px rgba(0,0,0,.5);\n        border-radius: 5px;\n    }\n    .block {\n        display: flex;\n        justify-content:center;\n        padding-top: 15px;\n        padding-bottom: 10px;\n        width: 100%;\n    }\n</style>\n"],"mappings":"AAwHA;EACAA,IAAA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACAC,aAAA,WAAAA,cAAAC,KAAA;MACA,KAAAC,WAAA,GAAAD,KAAA;MACA,KAAAE,mBAAA;IACA;IACAC,gBAAA,WAAAA,iBAAAC,GAAA;MACA,KAAAC,WAAA,GAAAD,GAAA;MACA,KAAAE,aAAA;IACA;IACAT,QAAA,WAAAA,SAAA;MAAA,IAAAU,KAAA;MACA,KAAAC,IAAA,CAAAC,YAAA;QACAC,IAAA,OAAAC,OAAA;QACAC,IAAA;MACA,GAAAC,IAAA